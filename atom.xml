---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Erik's Code</title>
  <link href="http://blog.dubbelboer.com/atom.xml" rel="self" />
  <link href="http://blog.dubbelboer.com/" />
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://blog.dubbelboer.com/</id>
  <author>
    <name>Erik Dubbelboer</name>
    <email>erik@dubbelboer.com</email>
  </author>
 
  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://blog.dubbelboer.com{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://blog.dubbelboer.com{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>
